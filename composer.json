{
    "name": "binary-cats/laravel-php-styles",
    "description": "Set of shared PHP style for PHP-CS-Fixer, based on Symphony (PRS12, PSR2) PHP Coding standard.",
    "homepage": "https://github.com/binary-cats/laravel-php-styles",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyrill Kalita",
            "email": "cyrill.kalita@gmail.com",
            "homepage": "https://github.com/binary-cats",
            "role": "Developer"
        }
    ],
    "keywords": [
        "binary-cats",
        "laravel",
        "php-style",
        "style"
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "illuminate/support":"^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "BinaryCats\\PhpStyles\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BinaryCats\\PhpStyles\\PhpStylesServiceProvider"
            ]
        }
    }
}
